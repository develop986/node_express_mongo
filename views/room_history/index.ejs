<div class="nav-align row">
  <div class="col-sm-3">
    <a href="/room_history/new">管理用</a>
  </div>
  <div class="col-sm-6">
    <h4>入退管理</h4>
  </div>
  <div class="col-sm-3">
  </div>
</div>
<table class="table">
  <thead>
    <tr>
      <th>履歴Id</th>
      <th>ユーザー</th>
      <th>入室項目</th>
      <th>開始時間</th>
      <th>終了時間</th>
      <th>解錠</th>
      <th>施錠</th>
      <th>作成時間</th>
      <th>更新時間</th>
      <th>修正</th>
      <th>削除</th>
    </tr>
  </thead>
  <tbody>
    <% room_history.forEach(room_history=> { %>
      <tr>
        <td>
          <%= room_history._id %>
        </td>
        <td>
          <% users.forEach(user=> { %>
            <% if (user._id.valueOf()===room_history.user._id.valueOf()) { %>
              <%= user.account %>
                <% } %>
                  <% }); %>
        </td>
        <td>
          <% rooms.forEach(room=> { %>
            <% if (room._id.valueOf()===room_history.room._id.valueOf()) { %>
              <%= room.name %>
                <% } %>
                  <% }); %>
        </td>
        <td>
          <%= room_history.room_start.toLocaleString() %>
        </td>
        <td>
          <% if (room_history.room_end !=null) { %>
            <%= room_history.room_end.toLocaleString() %>
              <% } %>
        </td>
        <td>
          <%= room_history.room_unlock %>
        </td>
        <td>
          <%= room_history.room_lock %>
        </td>
        <td>
          <%= room_history.createdAt.toLocaleString() %>
        </td>
        <td>
          <%= room_history.updatedAt.toLocaleString() %>
        </td>
        <td>
          <a href="<%= `/room_history/${room_history._id}/edit`%>">管理用</a>
        </td>
        <td>
          <a href="<%= `/room_history/${room_history._id}/delete?_method=DELETE` %>"
            onclick="return confirm('Are you sure you want to delete this record?')" a>管理用</a>
        </td>
      </tr>
      <% }); %>
  </tbody>
</table>